# FitSnap.AI Frontend Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_API_TIMEOUT=30000

# App Configuration
NEXT_PUBLIC_APP_NAME=FitSnap.AI
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=false

# AWS Configuration (if using S3 for image storage)
NEXT_PUBLIC_AWS_REGION=us-east-1
NEXT_PUBLIC_S3_BUCKET=fitsnap-images

# Authentication
NEXT_PUBLIC_AUTH_ENABLED=true
NEXT_PUBLIC_SESSION_TIMEOUT=3600000

# Image Processing
NEXT_PUBLIC_MAX_IMAGE_SIZE=10485760
NEXT_PUBLIC_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp

# AI Model Configuration
NEXT_PUBLIC_MODEL_ENDPOINT=/api/v1/analyze
NEXT_PUBLIC_CONFIDENCE_THRESHOLD=0.75